  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/jest",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "lint": "npx eslint . --ext .ts 'src/' 'food_truck_UI'",
    "lint:fix": "npx eslint . --ext .ts --fix",
    "database:setup": "sequelize db:drop; sequelize db:create;database:migrate",
    "database:migrate": "sequelize db:migrate",
    "migration:generate": "sequelize migration:generate --name",
    "prod": "rm -rf ./build/* && tsc -p ./tsconfig.prod.json && node ./build/server.js",
    "build-ui": "cd ./food_truck_UI && ng build",
    "run-ui-backend":"npm run build-ui && npm run dev"
  },
